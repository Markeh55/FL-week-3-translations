translate deutsch d17s05mo_ca636351:

    # op "Yep."
    op "Jep."

# game/code/d17/d17s05mo.rpy:396
translate deutsch d17s05mo_abb73d54:

    # op "\"How did I lose my virginity...\""
    op "\"Wie habe ich meine Jungfräulichkeit verloren...\""

